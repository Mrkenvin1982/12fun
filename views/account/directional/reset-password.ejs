<div class="main-page signup-page">
    <h2 class="title1">Reset Mật khẩu</h2>
    <div class="sign-up-row widget-shadow">
        <form action="#" method="post" id="formResetPassword">
            <div class="sign-u">
                <input type="text" name="username" placeholder="Tài khoản" required="">
                <div class="clearfix"> </div>
            </div>
            <div class="sign-u">
                <input type="text" name="qq" placeholder="CMND" required="">
                <div class="clearfix"> </div>
            </div>
            <div class="sign-u">
                <input type="email" name="email" placeholder="Email" required="">
                <div class="clearfix"> </div>
            </div>
            <div class="sign-u">
                <input type="password" name="pin" placeholder="Mã pin" required="">
            </div>
            <div class="sign-u" style="text-align: center">
                <div style="text-align: center">
                    <a id="btnReloadcaptcha">
                        <label for="exampleInputEmail1" class="pointer" id="lblCaptcha" style="text-align: center"></label>
                    </a>
                </div>
            </div>
            <div class="sign-u">
                <input type="captcha" name="svgcaptcha" placeholder="Mã xác nhận" required="">
            </div>

            <div class="sign-u" id="messageError" style="display: none; text-align: center">
                <label for="exampleInputEmail1" id="lblMessage" style="color: red; font-weight: bold">

                </label>
                <br>
                <br>
            </div>
            <div class="clearfix"> </div>
            <div class="sub_home" style="text-align: center">
                <input type="button" class="btn btn-primary" id="btnResetPassword" value="Reset Mật khẩu">
                <div class="clearfix"> </div>
            </div>
        </form>
    </div>
    <script src="js/controllers/reset-password.js"></script>
</div>